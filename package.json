{
  "name": "dynamic-calc",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.5.1",
  "description": "Interactive calculator for understanding money supply dynamics",
  "main": "script.js",
  "scripts": {
    "dev": "vite",
    "serve": "serve .",
    "build": "tsc && vite build",
    "deploy": "vercel deploy --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "keywords": [
    "economics",
    "calculator",
    "money-supply",
    "educational",
    "interactive"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^5.3.2",
    "chart.js": "^4.4.0",
    "mathjs": "^11.12.0"
  },
  "devDependencies": {
    "serve": "^14.2.1",
    "vercel": "^32.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "ts-jest": "^29.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  },
  "readme": "## Installation\n\n1. Install pnpm globally: `npm install -g pnpm@10.5.1`\n2. Install dependencies: `pnpm install`\n3. Start dev server: `pnpm dev`",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
